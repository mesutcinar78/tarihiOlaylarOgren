@model List<DunyaTarihi.Models.UyeViewModels>
@{
    ViewBag.Title = "Index";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>Kullanıcı Adı</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.KullaniciAdi</td>
                    <td>
                        @Html.ActionLink("Rol Ata", "RolAta", "Uye", new { id = item.ID }, new { @class = "btn btn-info" })
                    </td>
                </tr>
            }
        }
    </tbody>
</table>
